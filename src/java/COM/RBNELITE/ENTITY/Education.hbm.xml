<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="COM.RBNELITE.ENTITY.EducationDao" table="tbl_education_work">
       <id column="e_id" name="id">
          <generator class="increment"/>
      </id>
    <property column="school" name="school"/>
    <property column="collage" name="clg"/>
    <property column="graduation" name="graduation"/>
    <property column="other_graduation" name="othr_graduation"/>
    <property column="pg_clg" name="pg_clg"/>
    <property column="pg_degree" name="pg_degree"/>
    <property column="other_pg" name="othr_pg_degree"/>
    <property column="user_mail" name="user_mail"/>
  </class>
</hibernate-mapping>
