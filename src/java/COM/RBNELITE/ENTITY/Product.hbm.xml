<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="COM.RBNELITE.ENTITY.ProductDao" table="tbl_product">
      <id column="p_id" name="Id">
          <generator class="increment"/>
      </id>
      <property column="p_name" name="ProductName"/>
      <property column="p_details" name="ProductInfo"/>
       <property column="user_mail" name="user_mail"/>
      
  </class>
</hibernate-mapping>
