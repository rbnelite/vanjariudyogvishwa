<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="COM.RBNELITE.ENTITY.CommentDao" table="tbl_comment">
    <id column="cmt_id" name="Id">
      <generator class="increment"/>
    </id>
    <property column="comment" name="cmnt"/>
    <property column="comment_whom" name="cmt_whom"/>
    <property column="status_id" name="Status_id"/>

</class>
</hibernate-mapping>
