<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="COM.RBNELITE.ENTITY.OccupationDao" table="tbl_employment">
      <id column="emp_id" name="id">
          <generator class="increment"/>
      </id>
     <property column="company_name" name="org_name"/>
    <property column="occupation" name="occupation"/>
    <property column="product_details" name="product_details"/>
    <property column="no_of_emp" name="total_emp"/>
    <property column="emp_type" name="emp_type"/>
    <property column="annual_income" name="annual_income"/>
    <property column="user_mail" name="user_mail"/>
  </class>
</hibernate-mapping>
