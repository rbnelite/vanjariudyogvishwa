<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="COM.RBNELITE.ENTITY.AstroDao" table="tbl_astro">
      <id column="a_id" name="Id">
      <generator class="increment"/>
    </id>
    <property column="country" name="country"/>
    <property column="city" name="city"/>
    <property column="zodiac" name="zodiac"/>
    <property column="user_mail" name="user_mail"/>
  </class>
</hibernate-mapping>
