<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="COM.RBNELITE.ENTITY.RegistrationDao" table="registration">
      <id column="sno" name="id">
      <generator class="increment"/>
    </id>
    <property column="fname" name="fName"/>
    <property column="mname" name="mName"/>
    <property column="lname" name="lName"/>
    <property column="email_id" name="emailId"/>
    <property column="contact_no" name="contact"/>
    <property column="gender" name="gender"/>
    <property column="bdate" name="bDate"/>
    <property column="password" name="pass1"/>
    <property column="LastLogin" name="LastLogin"/>
  </class>
</hibernate-mapping>
